    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-orm": "0.44.6",
    "drizzle-seed": "0.3.1",
    "framer-motion": "12.23.24",
    "lucide-react": "0.545.0",
    "next": "15.5.4",
    "next-themes": "0.4.6",
    "pg": "8.16.3",
    "qrcode": "1.5.4",
    "react": "19.2.0",
