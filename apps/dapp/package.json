{
  "name": "dapp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "playwright install",
    "dev": "next dev --turbopack",
    "build": "next build",
    "preview": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write **/**/*.{html,css,scss,js,jsx,cjs,mjs,ts,tsx,mdx} --cache",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "db:studio": "drizzle-kit --config=drizzle/config.ts studio",
    "db:push": "drizzle-kit --config=drizzle/config.ts push",
    "db:create-migration": "drizzle-kit --config=drizzle/config.ts generate",
    "db:migrate": "drizzle-kit --config=drizzle/config.ts migrate",
    "db:seed": "tsx --env-file=.env drizzle/seeders/index.ts",
    "db-danger:reset": "tsx --env-file=.env drizzle/seeders/reset.ts"
  },
  "dependencies": {
